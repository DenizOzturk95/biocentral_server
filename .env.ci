# CI Environment Configuration
# Used by docker-compose.dev.yml for integration tests

# PostgreSQL Configuration
POSTGRES_DB=biocentral_test
POSTGRES_USER=biocentral
POSTGRES_PASSWORD=biocentral_ci_password
POSTGRES_PORT=5432

# Redis Configuration
REDIS_JOBS_PORT=6379

# SeaweedFS Configuration
SEAWEEDFS_FILER_PORT=8888

# Server Configuration
SERVER_PORT=9540
SERVER_DEBUG=true
MAX_FILE_SIZE=50000000

# Triton is disabled for CI (use ONNX instead)
USE_TRITON=false
