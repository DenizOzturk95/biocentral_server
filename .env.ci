# CI Environment Configuration
# Used by docker-compose.dev.yml for integration tests

# PostgreSQL Configuration

POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=embeddings_db
POSTGRES_USER=embeddingsuser
POSTGRES_PASSWORD=embeddingspwd
# Redis Configuration
REDIS_JOBS_PORT=6379

# SeaweedFS Configuration
SEAWEEDFS_FILER_PORT=8888

# Server Configuration
SERVER_PORT=9540
SERVER_DEBUG=true
MAX_FILE_SIZE=50000000

# Triton is disabled for CI (use ONNX instead)
USE_TRITON=false
